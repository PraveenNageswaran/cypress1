def parseTableRecord(tableRecord) {
    // Split the tableRecord string to extract key-value pairs
    def pairs = tableRecord.substring(1, tableRecord.size() - 1).split(", ")

    // Create a map to store key-value pairs
    def recordMap = [:]
    
    // Extract key-value pairs and add them to the map
    pairs.each { pair ->
        def keyValue = pair.split("=")
        recordMap[keyValue[0]] = keyValue[1]
    }
    
