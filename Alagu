def result = '<your_JSON_string_here>'

// Extract JSON objects using regular expressions
def jsonObjects = result.findAll(/\{[^{}]+\}/)

// Convert JSON objects to a JSON array
def jsonArray = jsonObjects.collect { new groovy.json.JsonSlurper().parseText(it) }

// Write JSON array to file
new File('output.json').withWriter { writer ->
    writer << new groovy.json.JsonBuilder(jsonArray).toPrettyString()
}
