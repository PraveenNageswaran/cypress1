import groovy.json.JsonSlurper

def json = '''
{
  "name": "pinky",
  "age": 25,
  "education": "BE",
  "Address": {
    "doorNo": 2,
    "wingNo": "A",
    "street": {
      "pincode": {
        "zip": 4000,
        "city": "chennai"
      }
    }
  }
}
'''

def parseAndPrintKeysAndValues(data) {
    def extractKeysAndValues(map, keyHeading, valueHeading) {
        map.each { key, value ->
            println "$keyHeading: $key"
            println "$valueHeading: $value"
            if (value instanceof Map) {
                extractKeysAndValues(value, keyHeading, valueHeading)
            }
        }
    }
    
    extractKeysAndValues(new JsonSlurper().parseText(data), "Key", "Value")
}

parseAndPrintKeysAndValues(json)
