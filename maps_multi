def isMap(obj) {
    return obj instanceof Map
}

def printMapValues(map) {
    map.each { key, value ->
        if (isMap(value)) {
            printMapValues(value)
        } else {
            println value
        }
    }
}

def parseAndPrint(data) {
    if (isMap(data)) {
        data.each { key, value ->
            if (isMap(value)) {
                parseAndPrint(value)
            } else {
                println value
            }
        }
    } else {
        println data
    }
}
