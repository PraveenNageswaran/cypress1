def json = '''
{
  "name": "pinky",
  "age": 25,
  "education": "BE",
  "Address": {
    "doorNo": 2,
    "wingNo": "A",
    "street": {
      "pincode": {
        "zip": 4000,
        "city": "chennai"
      }
    }
  }
}
'''

def isMap(obj) {
    return obj instanceof Map
}

def printMapValues(map, valuesList) {
    map.each { key, value ->
        if (isMap(value)) {
            printMapValues(value, valuesList)
        } else {
            valuesList.add(value)
        }
    }
}

def parseAndReturnValues(data) {
    def valuesList = []
    if (isMap(data)) {
        data.each { key, value ->
            if (isMap(value)) {
                printMapValues(value, valuesList)
            } else {
                valuesList.add(value)
            }
        }
    } else {
        valuesList.add(data)
    }
    return valuesList
}

def values = parseAndReturnValues(new groovy.json.JsonSlurper().parseText(json))
values.each { println it }
